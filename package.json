{
	"name": "test-tauri",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules"
	},
	"devDependencies": {
		"@material/animation": "^14.0.0",
		"@material/base": "^14.0.0",
		"@material/button": "^14.0.0",
		"@material/card": "^14.0.0",
		"@material/density": "^14.0.0",
		"@material/dialog": "^14.0.0",
		"@material/dom": "^14.0.0",
		"@material/elevation": "^14.0.0",
		"@material/focus-ring": "^14.0.0",
		"@material/icon-button": "^14.0.0",
		"@material/ripple": "^14.0.0",
		"@material/rtl": "^14.0.0",
		"@material/select": "^14.0.0",
		"@material/shape": "^14.0.0",
		"@material/theme": "^14.0.0",
		"@material/tokens": "^14.0.0",
		"@material/touch-target": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@smui/button": "7.0.0-beta.16",
		"@smui/card": "7.0.0-beta.16",
		"@smui/common": "7.0.0-beta.16",
		"@smui/select": "7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tauri-apps/cli": "^1.5.10",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.71.1",
		"smui-theme": "7.0.0-beta.16",
